{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ESNext",
    "module": "umd",
    "lib": [
      "ESNext",
      "dom"
    ],
    "jsx": "react",
    "noEmit": true,
    "sourceMap": true,
    "strict": true,
